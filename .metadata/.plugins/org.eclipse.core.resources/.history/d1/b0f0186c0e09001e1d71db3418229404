package jun12;

import java.util.Iterator;

//for
public class For05 {
	public static void main(String[] args) {	
	//for문을 무한하게 반복하기
		for (int i = 0 ; i <= 10; i++) { 
			System.out.println("반복합니다");
			i = 0;
		}
	
}
}
		//for문 () 안에 숫자없이 10번 반복하게 해주세요.
